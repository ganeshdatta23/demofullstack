# Production Environment Variables
NEXT_PUBLIC_APP_NAME=Apex Hospital Management System
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_ENVIRONMENT=production

# API Configuration (Update with your production API)
NEXT_PUBLIC_API_URL=https://your-backend-api.herokuapp.com
NEXT_PUBLIC_APP_URL=https://apex-hospital.netlify.app

# Google Gemini AI
NEXT_PUBLIC_GOOGLE_GEMINI_API_KEY=your_production_gemini_key

# Feature Flags
NEXT_PUBLIC_ENABLE_SYMPTOM_CHECKER=true
NEXT_PUBLIC_ENABLE_TELEMEDICINE=true
NEXT_PUBLIC_ENABLE_PAYMENTS=true
NEXT_PUBLIC_ENABLE_CHAT=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://apex-hospital.netlify.app
NEXT_PUBLIC_SITE_NAME=Apex Hospital